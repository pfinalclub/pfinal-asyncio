{
  "name": "pfinalclub/asyncio",
  "description": "An embeddable, composable, and reasonable PHP Async Runtime",
  "type": "library",
  "keywords": ["async", "asyncio", "fiber", "coroutine", "workerman", "concurrent", "event-loop"],
  "license": "MIT",
  "authors": [
    {
      "name": "PFinal",
      "email": "lampxiezi@gmail.com"
    }
  ],
  "version": "3.0.1",
  "require": {
    "php": ">=8.1",
    "workerman/workerman": "^4.1 || ^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "PfinalClub\\Asyncio\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "PfinalClub\\Asyncio\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage",
    "benchmark": "php benchmarks/run_all.php",
    "benchmark:task": "php benchmarks/01_task_creation.php",
    "benchmark:concurrent": "php benchmarks/02_concurrent_tasks.php",
    "benchmark:switch": "php benchmarks/03_context_switch.php",
    "benchmark:memory": "php benchmarks/04_memory_usage.php",
    "benchmark:real": "php benchmarks/05_real_world.php",
    "check-syntax": "find src -name '*.php' -exec php -l {} \\;",
    "demo": "php examples/demo.php",
    "example-basic": "php examples/basic.php",
    "example-concurrent": "php examples/concurrent.php",
    "example-timeout": "php examples/timeout.php",
    "example-advanced": "php examples/advanced.php"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
